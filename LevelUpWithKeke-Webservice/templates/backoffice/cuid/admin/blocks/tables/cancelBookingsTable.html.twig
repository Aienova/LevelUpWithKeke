<table id="table-resa3" class="table-resa hidden">
    <thead>
        <tr>
            <th>Code de réservation</th>
            <th>Date de réservation</th>
            <th>Adresse</th>
            <th>Coût totale</th>
            <th>Action</th>
            <th></th>
        </tr>
    </thead>
    <tbody>

        {% if  cancelBookings is defined %}
        {% if cancelBookings is not empty  %}
        {% for cancelBooking in cancelBookings %}

        <tr>
            <td>{{cancelBooking.code}}</td>
            <td>{{cancelBooking.bookingDate|date("d/m/Y h:i:s")}}</td>
            <td>{{cancelBooking.location}}</td>
            <td>{{cancelBooking.cost}}</td>
            <td><span data-action="/booking/{{cancelBooking.id}}" class="material-symbols-outlined decision-button">visibility</span></td>
  
        </tr>

        {% endfor %}
        {% else %}
                <tr><td colspan="7">Kein Ergebnis</td></tr>
        {% endif %}
        {% else %}
                <tr><td colspan="7">Kein Ergebnis</td></tr>
        {% endif %}

    </tbody>
</table>